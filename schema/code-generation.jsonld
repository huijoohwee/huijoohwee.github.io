{
  "@context": {
    "@base": "https://huijoohwee.github.io/schema/code-generation.jsonld",
    "@vocab": "https://huijoohwee.github.io/schema/code-generation#",
    "BoilerplateGenerator": "cg:BoilerplateGenerator",
    "CodeGenerator": "cg:CodeGenerator",
    "ContextAwareGenerator": "cg:ContextAwareGenerator",
    "FormatConverter": "cg:FormatConverter",
    "GenerationContext": "cg:GenerationContext",
    "GenerationResult": "cg:GenerationResult",
    "GenerationTemplate": "cg:GenerationTemplate",
    "GenerationWorkflow": "cg:GenerationWorkflow",
    "QualityValidator": "cg:QualityValidator",
    "RefactoringGenerator": "cg:RefactoringGenerator",
    "TemplateEngine": "cg:TemplateEngine",
    "agents": "https://huijoohwee.github.io/schema/agents.jsonld#",
    "ai": "https://huijoohwee.github.io/schema/ai-programming.jsonld#",
    "ca": "https://huijoohwee.github.io/schema/code-analysis.jsonld#",
    "cg": "https://huijoohwee.github.io/schema/code-generation#",
    "contextDepth": "cg:contextDepth",
    "core": "https://huijoohwee.github.io/schema/core.jsonld#",
    "dcterms": "http://purl.org/dc/terms/",
    "dmag": "https://huijoohwee.github.io/schema/dmag.jsonld#",
    "flow": "https://huijoohwee.github.io/schema/flow.jsonld#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "generationMetrics": "cg:generationMetrics",
    "generationStrategy": "cg:generationStrategy",
    "generatorType": "cg:generatorType",
    "qualityLevel": "cg:qualityLevel",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "sourceFormat": "cg:sourceFormat",
    "targetFormat": "cg:targetFormat",
    "templateType": "cg:templateType",
    "templateVariables": "cg:templateVariables",
    "validationRules": "cg:validationRules",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "cg:BoilerplateGenerator",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Factory",
      "ai:jjnhmLayer": "HBS",
      "cg:boilerplateTypes": [
        "project-scaffolding",
        "class-templates",
        "function-templates",
        "configuration-templates",
        "test-templates"
      ],
      "cg:kpiTarget": "boilerplate-generation-speed ≤100ms",
      "dcterms:description": "Generates standard boilerplate code patterns and project scaffolding",
      "rdfs:label": "Boilerplate Generator",
      "rdfs:subClassOf": "cg:CodeGenerator"
    },
    {
      "@id": "cg:CodeGenerationKPITargets",
      "@type": "ai:QualityValidation",
      "ai:targets": {
        "boilerplate-generation-speed": "≤100ms",
        "context-accuracy": "≥93%",
        "conversion-accuracy": "≥92%",
        "generation-quality": "≥90%",
        "generation-response-time": "≤300ms",
        "refactoring-quality": "≥95%",
        "template-compilation-time": "≤50ms",
        "template-processing-speed": "≥1000-templates/sec",
        "validation-accuracy": "≥96%"
      },
      "dcterms:description": "Key Performance Indicator targets for code generation systems",
      "rdfs:label": "Code Generation KPI Targets"
    },
    {
      "@id": "cg:CodeGenerationMetrics",
      "@type": "ai:QualityValidation",
      "ai:metrics": [
        {
          "@id": "cg:BoilerplateGenerationSpeedMetric",
          "ai:measurement": "boilerplate-generation-performance-testing",
          "ai:name": "Boilerplate Generation Speed",
          "ai:target": "≤100ms"
        },
        {
          "@id": "cg:ContextAccuracyMetric",
          "ai:measurement": "context-aware-generation-precision-testing",
          "ai:name": "Context Accuracy",
          "ai:target": "≥93%"
        },
        {
          "@id": "cg:ConversionAccuracyMetric",
          "ai:measurement": "format-conversion-precision-testing",
          "ai:name": "Format Conversion Accuracy",
          "ai:target": "≥92%"
        },
        {
          "@id": "cg:GenerationQualityMetric",
          "ai:measurement": "generated-code-quality-assessment",
          "ai:name": "Generation Quality",
          "ai:target": "≥90%"
        },
        {
          "@id": "cg:GenerationResponseTimeMetric",
          "ai:measurement": "code-generation-performance-benchmarking",
          "ai:name": "Generation Response Time",
          "ai:target": "≤300ms"
        },
        {
          "@id": "cg:RefactoringQualityMetric",
          "ai:measurement": "refactored-code-quality-validation",
          "ai:name": "Refactoring Quality",
          "ai:target": "≥95%"
        },
        {
          "@id": "cg:TemplateCompilationTimeMetric",
          "ai:measurement": "template-compilation-performance-testing",
          "ai:name": "Template Compilation Time",
          "ai:target": "≤50ms"
        },
        {
          "@id": "cg:TemplateProcessingSpeedMetric",
          "ai:measurement": "template-engine-performance-benchmarking",
          "ai:name": "Template Processing Speed",
          "ai:target": "≥1000-templates/sec"
        },
        {
          "@id": "cg:ValidationAccuracyMetric",
          "ai:measurement": "quality-validation-precision-testing",
          "ai:name": "Validation Accuracy",
          "ai:target": "≥96%"
        }
      ],
      "dcterms:description": "Comprehensive metrics for evaluating code generation performance",
      "rdfs:label": "Code Generation Metrics"
    },
    {
      "@id": "cg:CodeGenerationOntology",
      "@type": "core:Vocabulary",
      "ai:dmagPattern": "Module",
      "ai:jjnhmLayer": "JSONLD",
      "core:imports": [
        "https://huijoohwee.github.io/schema/core.jsonld",
        "https://huijoohwee.github.io/schema/ai-programming.jsonld",
        "https://huijoohwee.github.io/schema/agents.jsonld",
        "https://huijoohwee.github.io/schema/dmag.jsonld",
        "https://huijoohwee.github.io/schema/flow.jsonld",
        "https://huijoohwee.github.io/schema/code-analysis.jsonld"
      ],
      "core:license": "MIT",
      "core:version": "1.0.0",
      "dcterms:created": "2024-12-22",
      "dcterms:creator": "SOLO Coding Assistant",
      "dcterms:description": "Comprehensive ontology for AI-powered code generation, template systems, and format conversion",
      "dcterms:modified": "2024-12-22",
      "rdfs:label": "Code Generation Ontology"
    },
    {
      "@id": "cg:CodeGenerationWorkflow",
      "@type": "flow:CodeGenerationFlowPattern",
      "ai:dmagPattern": "Coordination",
      "ai:jjnhmLayer": "JDBL",
      "dcterms:description": "Comprehensive workflow for AI-powered code generation",
      "flow:workflowSteps": [
        {
          "@id": "cg:CodeGeneration",
          "flow:agent": "cg:ContextAwareGenerator",
          "flow:responsibility": "generate-code-with-context-awareness",
          "flow:stepType": "code-generation"
        },
        {
          "@id": "cg:ContextAnalysis",
          "flow:agent": "cg:GenerationContext",
          "flow:responsibility": "analyze-generation-context-and-requirements",
          "flow:stepType": "context-analysis"
        },
        {
          "@id": "cg:QualityValidation",
          "flow:agent": "cg:QualityValidator",
          "flow:responsibility": "validate-generated-code-quality",
          "flow:stepType": "quality-validation"
        },
        {
          "@id": "cg:ResultOptimization",
          "flow:agent": "cg:GenerationResult",
          "flow:responsibility": "optimize-and-finalize-generation-results",
          "flow:stepType": "result-optimization"
        },
        {
          "@id": "cg:TemplateSelection",
          "flow:agent": "cg:TemplateEngine",
          "flow:responsibility": "select-and-prepare-generation-templates",
          "flow:stepType": "template-selection"
        }
      ],
      "rdfs:label": "Code Generation Workflow"
    },
    {
      "@id": "cg:CodeGenerator",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Factory",
      "ai:jjnhmLayer": "JDBL",
      "cg:generatorCapabilities": [
        "template-based-generation",
        "context-aware-generation",
        "format-conversion",
        "quality-validation",
        "incremental-generation"
      ],
      "dcterms:description": "Base class for AI-powered code generators with template and context awareness",
      "rdfs:label": "Code Generator",
      "rdfs:subClassOf": "ai:AIAgent"
    },
    {
      "@id": "cg:ContextAwareGenerator",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Actor",
      "ai:jjnhmLayer": "NQDS",
      "cg:contextTypes": [
        "project-context-awareness",
        "user-intent-understanding",
        "historical-pattern-recognition",
        "dependency-awareness",
        "style-consistency"
      ],
      "cg:kpiTarget": "context-accuracy ≥93%",
      "dcterms:description": "Generates code with deep understanding of project context and user intent",
      "rdfs:label": "Context-Aware Generator",
      "rdfs:subClassOf": "cg:CodeGenerator"
    },
    {
      "@id": "cg:contextDepth",
      "@type": "rdf:Property",
      "dcterms:description": "Depth of context understanding in generation",
      "rdfs:domain": "cg:GenerationContext",
      "rdfs:label": "context depth",
      "rdfs:range": "xsd:integer"
    },
    {
      "@id": "cg:FormatConverter",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Adapter",
      "ai:jjnhmLayer": "JDBL",
      "cg:conversionTypes": [
        "language-to-language-conversion",
        "format-to-format-conversion",
        "syntax-transformation",
        "semantic-preservation",
        "structure-mapping"
      ],
      "cg:kpiTarget": "conversion-accuracy ≥92%",
      "dcterms:description": "Converts code between different formats and languages with semantic preservation",
      "rdfs:label": "Format Converter",
      "rdfs:subClassOf": "cg:CodeGenerator"
    },
    {
      "@id": "cg:GenerationContext",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Module",
      "ai:jjnhmLayer": "NQDS",
      "cg:contextTypes": [
        "project-context",
        "user-preferences-context",
        "environmental-context",
        "dependency-context",
        "style-guide-context"
      ],
      "dcterms:description": "Context information for code generation including project, user, and environmental context",
      "rdfs:label": "Generation Context",
      "rdfs:subClassOf": "ai:ProgrammingContext"
    },
    {
      "@id": "cg:generationMetrics",
      "@type": "rdf:Property",
      "dcterms:description": "Metrics and measurements from code generation",
      "rdfs:domain": "cg:GenerationResult",
      "rdfs:label": "generation metrics",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "cg:GenerationResult",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Module",
      "ai:jjnhmLayer": "NQDS",
      "cg:resultComponents": [
        "generated-code",
        "generation-metrics",
        "quality-assessment",
        "validation-status",
        "improvement-suggestions"
      ],
      "dcterms:description": "Result of code generation with generated code, metrics, and validation status",
      "rdfs:label": "Generation Result",
      "rdfs:subClassOf": "core:Concept"
    },
    {
      "@id": "cg:generationStrategy",
      "@type": "rdf:Property",
      "dcterms:description": "Strategy used for code generation",
      "rdfs:domain": "cg:CodeGenerator",
      "rdfs:label": "generation strategy",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "cg:GenerationTemplate",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Module",
      "ai:jjnhmLayer": "HBS",
      "cg:templateComponents": [
        "template-structure",
        "variable-definitions",
        "conditional-logic",
        "iteration-patterns",
        "partial-templates"
      ],
      "dcterms:description": "Template definition for code generation with variables and logic",
      "rdfs:label": "Generation Template",
      "rdfs:subClassOf": "core:Concept"
    },
    {
      "@id": "cg:GenerationWorkflow",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Coordination",
      "ai:jjnhmLayer": "JDBL",
      "cg:workflowSteps": [
        "context-analysis",
        "template-selection",
        "code-generation",
        "quality-validation",
        "result-optimization"
      ],
      "dcterms:description": "Orchestrated workflow for comprehensive code generation",
      "rdfs:label": "Generation Workflow",
      "rdfs:subClassOf": "ai:WorkflowOrchestration"
    },
    {
      "@id": "cg:generatorType",
      "@type": "rdf:Property",
      "dcterms:description": "Type of code generator being used",
      "rdfs:domain": "cg:CodeGenerator",
      "rdfs:label": "generator type",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "cg:qualityLevel",
      "@type": "rdf:Property",
      "dcterms:description": "Quality level of generated code",
      "rdfs:domain": "cg:GenerationResult",
      "rdfs:label": "quality level",
      "rdfs:range": "xsd:decimal"
    },
    {
      "@id": "cg:QualityValidator",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Resilience",
      "ai:jjnhmLayer": "JDBL",
      "cg:kpiTarget": "validation-accuracy ≥96%",
      "cg:validationTypes": [
        "syntax-validation",
        "semantic-validation",
        "style-validation",
        "performance-validation",
        "security-validation"
      ],
      "dcterms:description": "Validates generated code quality against standards and best practices",
      "rdfs:label": "Quality Validator",
      "rdfs:subClassOf": "ai:QualityValidation"
    },
    {
      "@id": "cg:RefactoringGenerator",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Service",
      "ai:jjnhmLayer": "JDBL",
      "cg:kpiTarget": "refactoring-quality ≥95%",
      "cg:refactoringTypes": [
        "extract-method-refactoring",
        "rename-refactoring",
        "move-class-refactoring",
        "inline-refactoring",
        "extract-interface-refactoring"
      ],
      "dcterms:description": "Generates refactored code with improved structure and maintainability",
      "rdfs:label": "Refactoring Generator",
      "rdfs:subClassOf": "cg:CodeGenerator"
    },
    {
      "@id": "cg:sourceFormat",
      "@type": "rdf:Property",
      "dcterms:description": "Source format for code conversion",
      "rdfs:domain": "cg:FormatConverter",
      "rdfs:label": "source format",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "cg:targetFormat",
      "@type": "rdf:Property",
      "dcterms:description": "Target format for code generation or conversion",
      "rdfs:domain": [
        "cg:CodeGenerator",
        "cg:FormatConverter"
      ],
      "rdfs:label": "target format",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "cg:TemplateEngine",
      "@type": "rdfs:Class",
      "ai:dmagPattern": "Service",
      "ai:jjnhmLayer": "HBS",
      "cg:kpiTarget": "template-processing-speed ≥1000-templates/sec",
      "cg:templateTypes": [
        "handlebars-templates",
        "mustache-templates",
        "jinja2-templates",
        "custom-templates",
        "dynamic-templates"
      ],
      "dcterms:description": "Advanced template engine for dynamic code generation with variable substitution",
      "rdfs:label": "Template Engine",
      "rdfs:subClassOf": "cg:CodeGenerator"
    },
    {
      "@id": "cg:templateType",
      "@type": "rdf:Property",
      "dcterms:description": "Type of template being used for generation",
      "rdfs:domain": "cg:GenerationTemplate",
      "rdfs:label": "template type",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "cg:templateVariables",
      "@type": "rdf:Property",
      "dcterms:description": "Variables used in template generation",
      "rdfs:domain": "cg:GenerationTemplate",
      "rdfs:label": "template variables",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "cg:validationRules",
      "@type": "rdf:Property",
      "dcterms:description": "Rules used for quality validation",
      "rdfs:domain": "cg:QualityValidator",
      "rdfs:label": "validation rules",
      "rdfs:range": "xsd:string"
    }
  ]
}
