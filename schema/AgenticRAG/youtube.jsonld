    "thumbnail_url": {
      "@type": "xsd:string",
      "rdfs:label": [
        { "@value": "Thumbnail URL", "@language": "en-us" },
        { "@value": "缩略图 URL", "@language": "zh-cn" }
      ],
      "required": true
    },
    "requested_language_code": {
