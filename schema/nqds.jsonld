  "@id": "https://huijoohwee.github.io/schema/nqds.jsonld",
  "@type": ["owl:Ontology", "schema:Dataset"],

  "meta": {
    "@type": "schema:Dataset",
    "title": "NQDS Canvas-Code Semantic Graph Vocabulary",
    "description": "N-Quads-style semantic payload vocabulary for WBSTCNVS canvas-code synchronization, bidirectional workflows, and visual-to-code transformation tracking",
    "creator": "https://huijoohwee.github.io",
    "license": "https://opensource.org/licenses/MIT",
    "conformsTo": [
      "https://www.w3.org/TR/rdf11-concepts/",
      "https://www.w3.org/TR/owl2-overview/",
      "https://www.w3.org/TR/prov-o/",
      "https://www.w3.org/TR/n-quads/"
    ],
    "versionInfo": "3.0.0",
    "created": "2024-01-01T00:00:00Z",
    "modified": "2025-10-23T02:00:00Z",
    "semVer": "20251023-v3.0.0",
    "maintainer": "https://huijoohwee.github.io",
    "jjnhm": {
          "v": "3.0.0",
          "lyr": "JSONLD",
          "cmplx": "C1-MVP-Prod",
          "gov": "Core-SG",
          "kpi": {
                "tok-dens": "≥7.0-c/t",
                "sem-clar": "≥97%",
                "parse-t": "≤0.5ms",
                "val-succ": "≥99%",
                "sch-comp": "100%"
          }
    },
    "layer": "semantic-payload",
    "responsibilities": [
      "canvas-code-relationships",
      "bidirectional-sync-flows",
      "visual-transformation-graphs",
      "sync-dependencies",
      "canvas-code-journey-tracing",
      "conflict-resolution-provenance"
    ]
  },

  "@graph": [
    {
      "@id": "nqds:SemanticGraph",
      "@type": "rdfs:Class",
      "rdfs:label": "Semantic Graph",
      "rdfs:comment": "Base class for semantic knowledge graphs and relationship networks"
    },
    {
      "@id": "nqds:Agent",
      "@type": "rdfs:Class",
      "rdfs:label": "Agent",
      "rdfs:comment": "Autonomous entity that performs actions and interacts with other agents"
    },
    {
      "@id": "nqds:Relationship",
      "@type": "rdfs:Class",
      "rdfs:label": "Relationship",
      "rdfs:comment": "Typed connection between entities in the semantic graph"
    },
    {
      "@id": "nqds:DataFlow",
      "@type": "rdfs:Class",
      "rdfs:label": "Data Flow",
      "rdfs:comment": "Movement and transformation of data through components"
    },
    {
      "@id": "nqds:KnowledgeNode",
      "@type": "rdfs:Class",
      "rdfs:label": "Knowledge Node",
      "rdfs:comment": "Node representing a concept, resource, or artifact"
    },
    {
      "@id": "nqds:SemanticEdge",
      "@type": "rdfs:Class",
      "rdfs:label": "Semantic Edge",
      "rdfs:comment": "Edge representing a relationship or inference link"
    },
    {
      "@id": "nqds:Dependency",
      "@type": "rdfs:Class",
      "rdfs:label": "Dependency",
      "rdfs:comment": "Requirement or prerequisite relationship between entities"
    },
    {
      "@id": "nqds:Transformation",
      "@type": "rdfs:Class",
      "rdfs:label": "Transformation",
      "rdfs:comment": "Process that converts data from one form to another"
    },
    {
      "@id": "nqds:Context",
      "@type": "rdfs:Class",
      "rdfs:label": "Context",
      "rdfs:comment": "Contextual metadata that gives meaning to semantic relationships"
    },
    {
      "@id": "nqds:Scope",
      "@type": "rdfs:Class",
      "rdfs:label": "Scope",
      "rdfs:comment": "Boundary or domain within which semantics apply"
    }
  ],

  "properties": [
    {
      "@id": "nqds:analyzes",
      "@type": "rdf:Property",
      "rdfs:label": "analyzes",
      "rdfs:comment": "Indicates that an agent analyzes or examines an entity",
