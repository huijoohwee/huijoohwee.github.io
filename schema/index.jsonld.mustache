{
  "@context": {
    "@base": "{{host}}/schema/base.jsonld",
    "@vocab": "{{host}}/schema/vocab.jsonld",
    "core":  "{{host}}/schema/core.jsonld",
    "uc":    "{{host}}/schema/uc.jsonld",
    "wf":    "{{host}}/schema/wf.jsonld",
    "kg":    "{{host}}/schema/kg.jsonld",
    "xsd":   "http://www.w3.org/2001/XMLSchema#",
    "rdf":   "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs":  "http://www.w3.org/2000/01/rdf-schema#",
    "nquads":"https://www.w3.org/TR/n-quads/#n-quads"
  },

  "@id": "core:Index:{{index_version}}",
  "@type": "core:Index",

  "meta": {
    "title": "{{meta_title}}",
    "description": "{{meta_description}}",
    "version": "{{index_version}}",
    "maintainer": "{{maintainer}}",
    "license": "{{license}}",
    "created": "{{created}}",
    "updated": "{{updated}}"
  },

  "entries": {
    "base": {
      "latest": "{{base_latest}}",
      "versions": [
        {{#base_versions}}"{{.}}"{{^last}}, {{/last}}{{/base_versions}}
      ]
    },
    "core": {
      "latest": "{{core_latest}}",
      "versions": [
        {{#core_versions}}"{{.}}"{{^last}}, {{/last}}{{/core_versions}}
      ]
    },
    "uc": {
      "latest": "{{uc_latest}}",
      "versions": [
        {{#uc_versions}}"{{.}}"{{^last}}, {{/last}}{{/uc_versions}}
      ]
    },
    "wf": {
      "latest": "{{wf_latest}}",
      "versions": [
        {{#wf_versions}}"{{.}}"{{^last}}, {{/last}}{{/wf_versions}}
      ]
    },
    "kg": {
      "latest": "{{kg_latest}}",
      "versions": [
        {{#kg_versions}}"{{.}}"{{^last}}, {{/last}}{{/kg_versions}}
      ]
    },
    "nqds-predicates": {
      "latest": "{{nqds_predicates_latest}}",
      "versions": [
        {{#nqds_predicates_versions}}"{{.}}"{{^last}}, {{/last}}{{/nqds_predicates_versions}}
      ]
    },
    "jjnhm": {
      "latest": "{{jjnhm_latest}}",
      "versions": [
        {{#jjnhm_versions}}"{{.}}"{{^last}}, {{/last}}{{/jjnhm_versions}}
      ]
    },
    "mmd": {
      "latest": "{{mmd_latest}}",
      "versions": [
        {{#mmd_versions}}"{{.}}"{{^last}}, {{/last}}{{/mmd_versions}}
      ]
    },
    "hbs": {
      "latest": "{{hbs_latest}}",
      "versions": [
        {{#hbs_versions}}"{{.}}"{{^last}}, {{/last}}{{/hbs_versions}}
      ]
    },
    "jdbl": {
      "latest": "{{jdbl_latest}}",
      "versions": [
        {{#jdbl_versions}}"{{.}}"{{^last}}, {{/last}}{{/jdbl_versions}}
      ]
    },
    "actions": {
      "latest": "{{actions_latest}}",
      "versions": [
        {{#actions_versions}}"{{.}}"{{^last}}, {{/last}}{{/actions_versions}}
      ]
    },
    "agents": {
      "latest": "{{agents_latest}}",
      "versions": [
        {{#agents_versions}}"{{.}}"{{^last}}, {{/last}}{{/agents_versions}}
      ]
    },
    "features": {
      "latest": "{{features_latest}}",
      "versions": [
        {{#features_versions}}"{{.}}"{{^last}}, {{/last}}{{/features_versions}}
      ]
    },
    "project-areas": {
      "latest": "{{project_areas_latest}}",
      "versions": [
        {{#project_areas_versions}}"{{.}}"{{^last}}, {{/last}}{{/project_areas_versions}}
      ]
    },
    "project-issues": {
      "latest": "{{project_issues_latest}}",
      "versions": [
        {{#project_issues_versions}}"{{.}}"{{^last}}, {{/last}}{{/project_issues_versions}}
      ]
    },
    "ui-ux-elements": {
      "latest": "{{ui_ux_elements_latest}}",
      "versions": [
        {{#ui_ux_elements_versions}}"{{.}}"{{^last}}, {{/last}}{{/ui_ux_elements_versions}}
      ]
    },
    "vocab": {
      "latest": "{{vocab_latest}}",
      "versions": [
        {{#vocab_versions}}"{{.}}"{{^last}}, {{/last}}{{/vocab_versions}}
      ]
    }
  }
}