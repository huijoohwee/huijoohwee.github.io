  "@id": "dmag:DistributedModularArchitectureGovernance",
  "@type": "jjnhm:ArchitectureGovernanceSchema",

  "meta": {
    "title": "DMAG: Distributed Modular Architecture Governance Schema",
    "description": "Comprehensive schema for distributed modular architecture governance patterns, enabling scalable multi-agent coordination and resilient system design",
    "versionInfo": "3.0.0",
    "maintainer": "JJNHM Architecture Team",
