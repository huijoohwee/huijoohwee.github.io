  },
  "usageExamples": [
    {
      "example": {
        "@context": [
          "https://www.w3.org/2013/json-ld-context/rdfa11",
          "https://huijoohwee.github.io/schema/base.jsonld",
          "https://huijoohwee.github.io/schema/uc.jsonld"
        ],
        "@id": "https://example.org/doc/1",
        "created": "2025-01-01T00:00:00Z",
        "description": "Demonstrates referencing uc concepts and core terms",
        "label": "Example Directive",
        "maintainer": "https://github.com/your-org",
        "status": "https://huijoohwee.github.io/schema/uc.jsonld#status",
        "type": "https://huijoohwee.github.io/schema/jdbl.jsonld#Directive"
      },
      "notes": "Import base and uc contexts, reference concepts by CURIE or full IRI, and use uc:trace for cross-layer tracing"
    }
  ]
}
