{
  "@context": "https://huijoohwee.github.io/schema/AgenticRAG/v1/context.jsonld",
  "@id": "rag:YouTubeTranscript",
  "@type": "rdfs:Class",
  "schemaVersion": "1.3.3",
  "rdfs:label": [
    { "@value": "YouTube Transcript", "@language": "en-us" },
    { "@value": "YouTube 转写文稿", "@language": "zh-cn" }
  ],
  "rdfs:comment": [
    {
      "@value": "Represents a YouTube transcript/captions payload ingested into AgenticRAG workflows. Extracted via native YouTube TimedText/InnerTube API fetch with fallback to video-analyzer (Whisper-based ASR over downloaded video/audio) or yt-dlp. Converted to Markdown/JSON for semantic analysis and GraphRAG indexing. Preserves rich metadata and time-aligned segments.",
      "@language": "en-us"
    },
    {
      "@value": "表示摄取到 AgenticRAG 工作流中的 YouTube 字幕/转写文稿载荷。通过原生 YouTube TimedText/InnerTube API 提取，支持回退到 video-analyzer（基于 Whisper 的 ASR，对下载的视频/音频进行转写）或 yt-dlp。转换为 Markdown/JSON，用于语义分析和 GraphRAG 索引。保留丰富元数据与时间对齐片段。",
      "@language": "zh-cn"
    }
  ],
  "required": [
    "title",
    "video_id",
    "source_url",
    "thumbnail_url",
    "requested_language_code",
    "requested_language",
    "requested_languages",
    "requested_start_s",
    "selected_language_code",
    "selected_language",
    "is_generated",
    "is_translatable",
    "translation_languages",
    "oembed",
    "start_s",
    "end_s",
    "duration_s",
    "segment_count",
    "generated_at_ms",
    "segments"
  ],
