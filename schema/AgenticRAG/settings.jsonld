      "rdfs:comment": [
        { "@value": "When enabled, PDF→Markdown conversion extracts page images and emits Markdown image tokens.", "@language": "en-us" },
        { "@value": "启用后，PDF→Markdown 转换会提取页面图片并生成 Markdown 图片标记。", "@language": "zh-cn" }
      ]
    },
    "pdfImportConversionMode": {
      "@type": "xsd:string",
      "rdfs:label": [
        { "@value": "PDF import: conversion preset", "@language": "en-us" },
        { "@value": "PDF 导入：转换预设", "@language": "zh-cn" }
      ],
      "rdfs:comment": [
        { "@value": "High-level preset for PDF→Markdown conversion (text-only, image-heavy, scan-ocr).", "@language": "en-us" },
        { "@value": "PDF→Markdown 转换的高层预设（仅文本、偏图片、扫描/OCR）。", "@language": "zh-cn" }
      ],
      "allowed_values": ["text-only", "image-heavy", "scan-ocr"]
    },
    "pdfImportMaxPages": {
      "@type": "xsd:integer",
      "rdfs:label": [
        { "@value": "PDF import: max pages", "@language": "en-us" },
        { "@value": "PDF 导入：最大页数", "@language": "zh-cn" }
      ]
    },
    "pdfImportMaxPdfBytes": {
      "@type": "xsd:integer",
