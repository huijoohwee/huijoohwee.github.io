      "rdfs:comment": [
        { "@value": "Default word-wrap behavior for Markdown Viewer and Presentation surfaces.", "@language": "en-us" },
        { "@value": "Markdown Viewer 与 Presentation 界面的默认自动换行行为。", "@language": "zh-cn" }
      ]
    },
    "markdownTextHighlight": {
      "@type": "xsd:boolean",
      "rdfs:label": [
        { "@value": "Markdown: text highlight", "@language": "en-us" },
        { "@value": "Markdown：文本高亮", "@language": "zh-cn" }
      ],
      "rdfs:comment": [
        { "@value": "Enables line-range highlight styling in Markdown Viewer and Presentation surfaces.", "@language": "en-us" },
        { "@value": "启用 Markdown Viewer 与 Presentation 界面的行区间高亮样式。", "@language": "zh-cn" }
      ]
    },
    "pdfImportIncludeImages": {
      "@type": "xsd:boolean",
      "rdfs:label": [
        { "@value": "PDF import: include images", "@language": "en-us" },
        { "@value": "PDF 导入：包含图片", "@language": "zh-cn" }
