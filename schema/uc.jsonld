{
  "@context": [
    "https://huijoohwee.github.io/schema/base.jsonld",
    {
      "Collection": "skos:Collection",
      "Concept": "skos:Concept",
      "ConceptScheme": "skos:ConceptScheme",
      "altLabel": "skos:altLabel",
      "broader": {
        "@id": "skos:broader",
        "@type": "@id"
      },
      "concepts": {
        "@container": "@index",
        "@id": "uc:hasConcept"
      },
      "definition": "skos:definition",
      "example": "core:example",
      "inScheme": {
        "@id": "skos:inScheme",
        "@type": "@id"
      },
      "narrower": {
        "@id": "skos:narrower",
        "@type": "@id"
      },
      "note": "skos:note",
      "notes": "rdfs:comment",
      "prefLabel": "skos:prefLabel",
      "related": {
        "@id": "skos:related",
        "@type": "@id"
      },
      "topConceptOf": {
        "@id": "skos:topConceptOf",
        "@type": "@id"
      },
      "uc": "https://huijoohwee.github.io/schema/uc.jsonld#"
    }
  ],
  "@graph": [
    {
      "@id": "uc:action",
      "@type": "skos:Concept",
      "definition": "A discrete operation or behavior that can be performed",
      "example": "Create, read, update, delete, process, validate",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Represents verbs or operations in the system",
      "prefLabel": "Action",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    },
    {
      "@id": "uc:constraint",
      "@type": "skos:Concept",
      "definition": "A limitation or restriction that must be observed",
      "example": "Business rules, validation rules, access permissions",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Defines boundaries and limitations within the system",
      "prefLabel": "Constraint",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    },
    {
      "@id": "uc:context",
      "@type": "skos:Concept",
      "definition": "The circumstances or environment in which something exists or occurs",
      "example": "Development environment, user session, project scope",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Provides situational information for proper interpretation",
      "prefLabel": "Context",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    },
    {
      "@id": "uc:entity",
      "@type": "skos:Concept",
      "definition": "A fundamental unit of existence or identity within a system",
      "example": "A user, a document, a process, or any identifiable object",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Base concept for all identifiable objects in the system",
      "prefLabel": "Entity",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    },
    {
      "@id": "uc:layer",
      "@type": "skos:Concept",
      "definition": "A level of abstraction or organization within a hierarchical structure",
      "example": "Presentation layer, business logic layer, data layer",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Represents different levels of abstraction in system architecture",
      "prefLabel": "Layer",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    },
    {
      "@id": "uc:pattern",
      "@type": "skos:Concept",
      "definition": "A reusable solution or template for common problems",
      "example": "Design patterns, architectural patterns, workflow patterns",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Represents proven solutions that can be applied repeatedly",
      "prefLabel": "Pattern",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    },
    {
      "@id": "uc:relationship",
      "@type": "skos:Concept",
      "definition": "A connection or association between two or more entities",
      "example": "Parent-child, dependency, ownership, membership",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Defines how entities are connected to each other",
      "prefLabel": "Relationship",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    },
    {
      "@id": "uc:state",
      "@type": "skos:Concept",
      "definition": "A condition or mode of being at a particular point in time",
      "example": "Active, inactive, pending, completed, error",
      "inScheme": "https://huijoohwee.github.io/schema/uc.jsonld",
      "note": "Represents the current condition of an entity",
      "prefLabel": "State",
      "topConceptOf": "https://huijoohwee.github.io/schema/uc.jsonld"
    }
  ],
  "@id": "https://huijoohwee.github.io/schema/uc.jsonld",
  "@type": [
    "skos:ConceptScheme",
    "schema:Dataset"
  ],
