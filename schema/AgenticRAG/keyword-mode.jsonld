{
  "@context": "https://huijoohwee.github.io/schema/AgenticRAG/v1/context.jsonld",
  "@id": "rag:KeywordMode",
  "@type": "rdfs:Class",
  "schemaVersion": "1.0.0",
  "rdfs:comment": [
    {
      "@value": "Defines the Keyword Mode graph generation logic, including Subject-Verb-Object triple extraction and statistical co-occurrence.",
      "@language": "en-us"
    },
    {
      "@value": "定义关键词模式图生成逻辑，包括主谓宾三元组提取和统计共现。",
      "@language": "zh-cn"
    }
  ],
  "rdfs:label": [
    { "@value": "Keyword Mode", "@language": "en-us" },
    { "@value": "关键词模式", "@language": "zh-cn" }
  ],
  "propertyDefinitions": {
    "nodes": {
      "type": "Keyword",
      "properties": {
        "keyword:role": {
          "description": "Role of the node in the text (subject, object, entity)",
          "enum": ["subject", "object", "entity"]
        },
        "keyword:pagerank": {
          "description": "PageRank centrality score within the keyword graph"
        },
        "visual:community": {
          "description": "Community ID derived from connected components"
        }
      }
    },
    "edges": {
      "properties": {
        "keyword:predicate": {
          "description": "Predicate label inferred from text between mentions"
        },
        "keyword:verbLike": {
          "description": "Whether the predicate is a verb-like token"
        }
      }
    }
  }
}
