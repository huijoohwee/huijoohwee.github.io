    "@id": "agent:ai-code-analyzer",
    "@type": "core:AIAgent",
    "skos:prefLabel": "AI Code Analyzer Agent",
    "skos:definition": "Intelligent agent for comprehensive code analysis, quality assessment, and improvement suggestions",
    "core:capability": [
      "best-practices-analysis",
      "performance-analysis",
      "accessibility-analysis",
      "security-analysis",
      "refactoring-suggestions",
      "code-metrics-calculation"
    ],
    "core:analysisType": [
      "static-analysis",
      "dynamic-analysis",
      "semantic-analysis",
      "pattern-recognition"
    ],
    "core:jjnhmLayer": "JDBL",
    "core:dmagPattern": "service-factory",
    "activeVerbs": [
      "analyze-code",
      "detect-patterns",
      "suggest-improvements",
      "calculate-metrics"
    ],
    "kpiTarget": "analysis-accuracy≥95%",
    "characteristics": [
      "intelligent",
      "comprehensive",
      "actionable"
    ]
  },
  {
    "@id": "agent:ai-code-generator",
    "@type": "core:AIAgent",
    "skos:prefLabel": "AI Code Generator Agent",
    "skos:definition": "Intelligent agent for automated code generation from specifications, templates, and patterns",
    "core:capability": [
      "typescript-generation",
      "javascript-generation",
      "tsx-generation",
      "markdown-generation",
      "html-generation",
      "json-ld-generation"
    ],
    "core:targetFormat": [
      "typescript",
      "javascript",
      "tsx",
      "markdown",
      "html",
      "json-ld"
    ],
    "core:jjnhmLayer": "HBS",
    "core:dmagPattern": "module-of-classes",
    "activeVerbs": [
      "generate-code",
      "apply-templates",
      "transform-specifications",
      "optimize-output"
    ],
    "kpiTarget": "generation-quality≥90%",
    "characteristics": [
      "adaptive",
      "template-driven",
      "format-aware"
    ]
  },
  {
    "@id": "agent:ai-canvas-converter",
    "@type": "core:AIAgent",
    "skos:prefLabel": "AI Canvas Converter Agent",
    "skos:definition": "Intelligent agent for bidirectional conversion between visual canvas representations and code",
    "core:capability": [
      "canvas-to-code-conversion",
      "code-to-canvas-conversion",
      "visual-synchronization",
      "layout-optimization"
    ],
    "core:conversionDirection": [
      "bidirectional",
      "canvas-to-code",
      "code-to-canvas"
    ],
    "core:jjnhmLayer": "NQDS",
    "core:dmagPattern": "class-as-adapter",
    "activeVerbs": [
      "convert-representation",
      "synchronize-views",
      "optimize-layout",
      "maintain-consistency"
    ],
    "kpiTarget": "conversion-accuracy≥92%",
    "characteristics": [
      "bidirectional",
      "visual",
      "synchronized"
    ]
  },
